public class Driver{
    public static void main(String[] args) {
	MyLinkedList l = new MyLinkedList();
	l.add("test",0);
	l.add("moretest",1);
	l.add("lesstest",0);
	l.add("lasttest",3);
    
	System.out.println(l);
	l.set(2,"edit");
	System.out.println(l.get(2));
	System.out.println(l);
	l.remove(2);
	System.out.println(l);
	l.remove(0);
	System.out.println(l);
    }
}
